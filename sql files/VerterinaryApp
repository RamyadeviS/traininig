SELECT Owner_id from owner ORDER BY Owner_id DESC;

SELECT Vet_id,V_fName,V_lName,Salary_Per_Mon from Vet where Salary_per_Mon >8000;
truncate table Pet;
ALTER table Pet add Appointment_date VARCHAR(23);


